{
  "expo": {
    "name": "HelloWorld",
    "slug": "HelloWorld",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": ["ios", "android"],
    "scheme": "HelloWorld",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.HelloWorld"
    },
    "android": {
      "package": "com.helloworld",
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "react-native-keys",
      "expo-font"
    ]
  }
}
